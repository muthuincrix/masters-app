{
   "version": 2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node",
           "functions": {
               "controller/**/*":{
                     "maxDuration": 30
               }
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/index.js"
        }

    ]
}
